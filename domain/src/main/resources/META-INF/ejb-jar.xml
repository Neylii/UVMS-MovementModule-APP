<ejb-jar
	xmlns = "http://java.sun.com/xml/ns/javaee"
	version = "3.1"
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
	<interceptors>
		<interceptor>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor>
	</interceptors>
	
	<assembly-descriptor>
		<interceptor-binding>
			<ejb-name>IncomingMovementBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MovementBatchModelBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MovementDomainModelBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MovementProcessorBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MovementSearchGroupDomainModelBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>SegmentBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>TempMovementDomainModelBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>		
	</assembly-descriptor>	
</ejb-jar>